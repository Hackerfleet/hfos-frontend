{
    "extends": "eslint-config-defaults",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "mocha": true,
        "jasmine": true
    },
    "globals": {
        "angular": false,
    },
    "ecmaFeatures": {
        "modules": true
    },
    "rules": {
        "eqeqeq": 1,
        "no-console": 0,
        "no-undef": 1,
        "no-unused-vars": 1
    }
}
