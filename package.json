{
  "name": "hfos-frontend",
  "version": "1.1.0",
  "description": "Enhanced testing of Angular JS 1.X applications using ES6 modules",
  "repository": "https://github.com/hackerfleet/hfos-frontend",
  "main": "src/app.js",
  "scripts": {
    "start": "webpack-dev-server --TARGET=DEV",
    "build": "webpack --display-chunks --display-reasons --display-error-details --display-modules",
    "dist": "webpack --TARGET=DIST",
    "server_build": "node server.js",
    "server_dist": "node server.js --TARGET=DIST",
    "lint": "eslint src/",
    "mocha": "mocha --compilers js:babel-register ./src/**/*.test.js",
    "watch": "mocha --compilers js:babel-register -w ./src/**/*.test.js",
    "karma": "karma start",
    "test": "npm run lint && npm run mocha && karma start --single-run",
    "ci": "npm run lint && npm run mocha && npm run dist"
  },
  "author": "riot@hackerfleet.org",
  "license": "GPL-3.0",
  "dependencies": {
    "@kariudo/angular-fullscreen": "^1.0.1",
    "angular": "1.5",
    "angular-animate": "1.5",
    "angular-aria": "^1.5.0",
    "angular-cookies": "^1.5.0",
    "angular-gridster": "^0.13.3",
    "angular-local-storage": "^0.2.6",
    "angular-md5": "^0.1.10",
    "angular-sanitize": "^1.5.3",
    "angular-schema-form": "^0.8.12",
    "angular-schema-form-bootstrap": "^0.2.0",
    "angular-strap": "^2.3.7",
    "angular-touch": "^1.5.7",
    "angular-ui-router": "0.2.15",
    "bootstrap": "3.3.5",
    "bootstrap-additions": "^0.3.1",
    "favico.js": "^0.3.10",
    "font-awesome": "^4.6.1",
    "humanize-duration": "^3.7.0",
    "jquery": "2.1.4",
    "json-loader": "^0.5.4",
    "lodash": "3.10.1",
    "ng-draggable": "^1.0.0"
  },
  "devDependencies": {
    "angular-mocks": "1.5",
    "awesome-typescript-loader": "0.11.3",
    "babel-core": "6.5",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-runtime": "6.4.3",
    "babel-preset-es2015": "6.3.13",
    "babel-register": "6.5",
    "babel-runtime": "6.5",
    "chai": "3.2.0",
    "chalk": "1.1.0",
    "clean-webpack-plugin": "0.1.3",
    "connect": "3.4.0",
    "css-loader": "0.15.6",
    "eslint": "1.10.3",
    "eslint-config-defaults": "8.0.2",
    "file-loader": "0.8.4",
    "html-loader": "0.3.0",
    "html-webpack-plugin": "1.6.1",
    "jasmine-core": "2.3.4",
    "karma": "0.13.19",
    "karma-chrome-launcher": "0.2.0",
    "karma-jasmine": "0.3.6",
    "karma-mocha-reporter": "1.1.1",
    "karma-webpack": "1.7.0",
    "minimist": "1.1.3",
    "mocha": "2.2.5",
    "ng-annotate-loader": "^0.1.0",
    "ngtemplate-loader": "1.3.0",
    "open-browser-webpack-plugin": "0.0.2",
    "sass-loader": "2.0.1",
    "node-sass": "3.4.2",
    "serve-static": "1.10.0",
    "sinon": "1.17.1",
    "source-map-loader": "^0.1.5",
    "style-loader": "0.12.3",
    "typescript": "1.5.3",
